<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.campjoy.identitree.starter"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="CJApplication">
        <activity android:name=".activities.SplashActivity">
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.campjoy.identitree.starter.MainActivity" android:label="@string/app_name">
        </activity><activity android:name=".activities.TreeInfoActivity">
        </activity>
        <activity
            android:name="org.campjoy.identitree.starter.activities.QuestionActivity"
            android:label="@string/title_activity_question" >
        </activity>
        

    </application>

</manifest>
